generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Habit {
  id               Int     @id @default(autoincrement())
  habit            String
  habitInformation String?
  target           Int
  iconName         String?
  iconCode         String?
  comments         String?
}

model Weather {
  id   Int    @id @default(autoincrement())
  city String
}
